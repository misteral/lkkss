<%= simple_form_for(@cost) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date_start, :label => "укажите начало периода" %>
    <%= f.input :date_end, :label => "укажите конец периода" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "произвести вычесления" %>
  </div>
<% end %>
