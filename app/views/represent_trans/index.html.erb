<% @represent_trans.each do |represent_tran| %>
  <tr>
    <td><%= represent_tran.first_name %></td>
    <td><%= represent_tran.last_name %></td>
    <td><%= represent_tran.middle_name %></td>
    <td><%= represent_tran.job %></td>
    <td><%= represent_tran.organization %></td>
    <td><%= link_to 'Show', represent_tran %></td>
    <td><%= link_to 'Edit', edit_represent_tran_path(represent_tran) %></td>
    <td><%= link_to 'Destroy', represent_tran, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

